<body>
<h1>List of Students</h1>

<table class="table">
    <thead>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Email</th>
        <th>Age</th>
        <th>Classroom</th>
       <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    {% for student in list %}
        <tr>
            <td>{{ student.id }}</td>
            <td>{{ student.name }}</td>
            <td>{{ student.email }}</td>
            <td>{{ student.age }}</td>
            <td>{{ student.classroom }}</td>
             <td>
                <a href="{{ path('edit', {'id': student.id}) }}" class="btn btn-primary">Edit</a>
                <a href="{{ path('delete', {'id': student.id}) }}" class="btn btn-danger">Delete</a>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<a href="{{ path('add') }}" class="btn btn-success">Add Student</a>
</body>